{
  "name": "autonomais",
  "version": "0.0.0-development",
  "description": "An approachable way to define multiple, collaborative AI agents",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "build": "tsc",
    "commit": "cz",
    "demo": "ts-node src/autonomais.ts ./examples/calculator.yaml",
    "dev": "ts-node src/autonomais.ts",
    "semantic-release": "semantic-release",
    "test": "jest",
    "update scripts": "npx npm-check-updates -u"
  },
  "dependencies": {
    "@langchain/community": "^0.0.47",
    "@langchain/core": "^0.1.57",
    "@langchain/langgraph": "0.0.12",
    "@langchain/openai": "^0.0.28",
    "yaml": "^2.4.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.5.12",
    "cz-conventional-changelog": "^3.3.0",
    "jest": "^29.7.0",
    "semantic-release": "^23.0.8",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/npm",
    "@semantic-release/git"
  ],
  "release": {
    "branches": [
      "main"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nyvyn/autonomais.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
