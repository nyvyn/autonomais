{
  "name": "autonomais",
  "version": "0.0.0-semantic-release",
  "description": "An approachable way to define multiple, collaborative AI agents",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "build": "tsc",
    "commit": "git add -p && cz",
    "demo": "ts-node src/autonomais.ts ./examples/calculator.yaml",
    "dev": "ts-node src/autonomais.ts",
    "semantic-release": "semantic-release",
    "test": "jest",
    "update scripts": "npx npm-check-updates -u"
  },
  "dependencies": {
    "@langchain/community": "^0.0.57",
    "@langchain/core": "^0.1.63",
    "@langchain/langgraph": "0.0.15",
    "@langchain/openai": "^0.0.31",
    "json5": "^2.2.3",
    "yaml": "^2.4.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.5.12",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "semantic-release": "^23.1.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/npm",
    "@semantic-release/git"
  ],
  "release": {
    "branches": [
      "main"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nyvyn/autonomais.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  }
}
